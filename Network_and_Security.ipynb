        "import hashlib\n",
        "def gen_md5():\n",
        "  name=\"Harsh\"\n",
        "  result = name.encode(\"utf-8\")\n",
        "\n",
        "  gen_hash = hashlib.md5(result)\n",
        "  md5_output = gen_hash.hexdigest()\n",
        "\n",
        "  print(md5_output)\n",
        "\n",
        "  return\n",
        "\n",
        "gen_md5()"
